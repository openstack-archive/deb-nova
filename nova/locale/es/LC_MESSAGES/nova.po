msgstr ""
"%(desc)r\n"
"comando: %(cmd)r\n"
"código de salida: %(code)r\n"
"stdout: %(stdout)r\n"
"stderr: %(stderr)r"

#, python-format
msgid "%(err)s"
msgstr "%(err)s"

